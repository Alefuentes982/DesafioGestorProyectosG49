<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Titulo del proyecto</th>
            <th scope="col">Fecha de inicio</th>
            <th scope="col">Fecha de termino</th>
            <th scope="col">Estado actual</th>
            <th scope="col">Descripcion</th>
        <tr>
    </thead>
    <tbody>
        <% i = 1 %>
        <% @projects.each do |project_entry| %>
        <tr>
            <th scope="row"> <%= i %> </th>
            <td> <%= project_entry.project_name %> </td>
            <td> <%= project_entry.start_date %> </td>
            <td> <%= project_entry.end_date %> </td>
            <td> <%= project_entry.status %> </td>
            <td> <%= project_entry.description %> </td>
        </tr>
        <% i += 1 %>
        <% end %>
    </tbody>
</table>

